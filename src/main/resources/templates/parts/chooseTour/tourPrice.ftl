                            <div class="tourPriceTableTdDataName" id="tourPriceTableTdDataDuration">
                                ${tour.duration}
                            </div>
                        </div>
                        <div class="tourPriceTableTdAuto">
                            <div class="tourPriceTableTdAutoLogo">
                                <img src="img/car2.png" alt="e">
